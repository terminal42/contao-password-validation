{
    "name": "terminal42/contao-password-validation",
    "type": "contao-bundle",
    "description": "A Contao bundle that validates user passwords against your password policy",
    "homepage": "https://github.com/terminal42/contao-password-validation",
    "keywords": ["contao", "password", "validation"],
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "support": {
        "issues": "https://github.com/terminal42/contao-password-validation/issues",
        "source": "https://github.com/terminal42/contao-password-validation",
        "forum": "https://community.contao.org"
    },
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^4.13 || ^5.0",
        "paragonie/hidden-string": "^1.0",
        "symfony/config": "^5.0 || ^6.0",
        "symfony/dependency-injection": "^5.0 || ^6.0",
        "symfony/http-client": "^5.0 || ^6.0",
        "symfony/http-kernel": "^5.0 || ^6.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.7",
        "terminal42/contao-build-tools": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\PasswordValidationBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\PasswordValidationBundle\\ContaoManager\\Plugin"
    }
}
